---
interface Props {
  title: string;
  body: string;
  href: string;
}

const { href, title, body } = Astro.props;
---

<button>
  <a href="/contact">資料請求</a>
</button>
<style>
  button {
    width: 280px;
    height: 50px;
    background-color: #f5a623;
    color: #fff;
    border-radius: 25px;
    padding: 8px 16px;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
  }
</style>
