---
interface Props {
  title: string;
  body: string;
  href: string;
}

const { href, title, body } = Astro.props;
---

<button>
  <a href="/contact">料金を見てみる</a>
</button>
<style>
  button {
    width: 280px;
    height: 50px;
    background-color: transparent;
    color: #fff;
    border: 1px solid #fff;
    border-radius: 25px;
    padding: 8px 16px;
    font-size: 16px;
    font-weight: bold;
    text-align: center;
  }
</style>
